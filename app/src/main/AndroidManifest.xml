<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.dev.personalassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ManageExpenseTagsActivity"
            android:theme="@style/Theme.AppCompat.Dialog.Alert" />
        <activity android:name=".AddEditExpensesActivity" />
        <activity android:name=".MyInformationActivity" />
        <activity android:name=".ShowBankListActivity" />
        <activity android:name=".ShowBankDetailsActivity" />
        <activity android:name=".ShowDocumentsListActivity" />
        <activity android:name=".ShowDocumentDetailsActivity"></activity>
    </application>

</manifest>